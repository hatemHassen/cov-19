{% extends 'front/base.html.twig' %}
{% trans_default_domain "home" %}
{% block content %}
    <div class="home">
        <!--====== HERO PART START ======-->
        <section id="home" class="hero-area bg_cover" style="background: url('{{ asset('images/moknine.jpg') }}') ;width: auto; background-size: 100%; background-position: 0;">
            <div class="container">
                <div class="row align-items-center">
                    <div class="col-xl-6 col-lg-6">
                        <div class="hero-content">
                            <h2 class="wow fadeInUp" data-wow-delay=".2s">{{'home.header.title'|trans}}</br> {{'home.header.subtitle'|trans}}</h2>
                            <p class="wow fadeInUp" data-wow-delay=".4s">{{'home.header.description'|trans}}</p>
                        </div>
                    </div>
                    {#                <div class="col-xl-6 col-lg-6">#}
                    {#                    <div class="hero-img">#}
                    {#                        <img src="{{ asset('images/moknine_la_nuit.jpg') }}" width="150" alt="" class="wow fadeInRight" data-wow-delay=".2s">#}
                    {#                        <img src="{{ asset('images/rondpoint.jpg') }}" width="150" alt="" class="img-screen screen-1 wow fadeInUp" data-wow-delay=".25s">#}
                    {#                        <img src="{{ asset('images/5septembre.jpg') }}" width="150" alt="" class="img-screen screen-2 wow fadeInUp" data-wow-delay=".3s">#}
                    {#                        <img src="{{ asset('images/entre.jpg') }}" width="150" alt="" class="img-screen screen-3 wow fadeInUp" data-wow-delay=".35s">#}
                    {#                    </div>#}
                    {#                </div>#}
                </div>
            </div>
        </section>
        <section id="keyNumbers" class="feature-area pt-120">
            <div class="container">
                <div class="section-title">
                    <h2 class="mb-60 wow fadeInUp" data-wow-delay=".2s">{{'home.sections.keyNumbers.title'|trans}}</h2>
                </div>
                <div class="row">
                    <div class="col-xl-3 col-lg-3 col-md-6">
                        <div class="single-feature item-1 wow fadeInUp" data-wow-delay=".2s">
                            <div class="feature-icon icon-style gradient-1">
                                <i class="lni lni-protection"></i>
                            </div>
                            <div class="feature-content">
                                <h4>{{'home.sections.keyNumbers.totalCases'|trans}}</h4>
                                <p>5</p>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-3 col-lg-3 col-md-6">
                        <div class="single-feature item-2 wow fadeInUp" data-wow-delay=".4s">
                            <div class="feature-icon icon-style gradient-2">
                                <i class="lni lni-notepad"></i>
                            </div>
                            <div class="feature-content">
                                <h4>{{'home.sections.keyNumbers.actifCases'|trans}}</h4>
                                <p>2</p>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-3 col-lg-3 col-md-6">
                        <div class="single-feature item-3 wow fadeInUp" data-wow-delay=".6s">
                            <div class="feature-icon icon-style gradient-3">
                                <i class="lni lni-dashboard"></i>
                            </div>
                            <div class="feature-content">
                                <h4>{{'home.sections.keyNumbers.healedCases'|trans}}</h4>
                                <p>10</p>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-3 col-lg-3 col-md-6">
                        <div class="single-feature item-4 wow fadeInUp" data-wow-delay=".8s">
                            <div class="feature-icon icon-style gradient-4">
                                <i class="lni lni-laptop-phone"></i>
                            </div>
                            <div class="feature-content">
                                <h4>{{'home.sections.keyNumbers.deathCases'|trans}}</h4>
                                <p>0</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <section id="survey" class="how-work-area pt-130">
            <div class="container">
                <div class="row">
                    <div class="text-center">
                        <h2>{{'home.sections.quizz.title'|trans}}</h2>
                        <p>{{'home.sections.quizz.description'|trans}}</p>
                        </br>
                        <a href="{{path('patient_create')}}" class="main-btn btn-hover wow fadeInUp" data-wow-delay=".45s" style="visibility: visible; animation-delay: 0.45s; animation-name: fadeInUp;">{{'home.sections.quizz.start'|trans}}</a>

                    </div>
                </div>
            </div>
        </section>
        <section id="situation" class="situation-area pt-120">
            <div class="container">
                <div class="row">
                    <div class="col-xl-6 col-lg-8 col-md-10 mx-auto">
                        <div class="section-title text-center">
                            <h2 class="mb-30 wow fadeInUp" data-wow-delay=".2s">{{'home.sections.situation.title'|trans}}</h2>
                            <p class="mb-50 wow fadeInUp" data-wow-delay=".4s">{{'home.sections.situation.description'|trans}}</p>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-12 mx-auto">
                        <div class="situation-nav">
                            <div class="row">
                                <div class="col-xl-4 col-lg-4 col-md-6">
                                    <div class="single-chart text-center wow fadeInUp" data-wow-delay=".2s">
                                        <canvas id="firstChart" style="max-width: 500px;"></canvas>
                                    </div>
                                </div>
                                <div class="col-xl-4 col-lg-4 col-md-6">
                                    <div class="single-chart text-center active wow fadeInUp" data-wow-delay=".4s">
                                        <canvas id="secondChart" style="max-width: 500px;"></canvas>
                                    </div>
                                </div>
                                <div class="col-xl-4 col-lg-4 col-md-6">
                                    <div class="single-chart text-center wow fadeInUp" data-wow-delay=".2s">
                                        <canvas id="thirdChart" style="max-width: 500px;"></canvas>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
        </section>
          <section id="contact" class="contact-area pt-130">
            <div class="container">
                <div class="row">
                    <div class="col-xl-7 col-lg-10 mx-auto">
                        <div class="section-title text-center">
                            <h2 class="mb-60 wow fadeInUp" data-wow-delay=".2s">{{'home.sections.contact.faq.title'|trans}}</h2>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-xl-6 col-lg-6">
                        <div class="faq-wrapper">
                            <h3 class="mb-40 wow fadeInUp" data-wow-delay=".2s">{{'home.sections.contact.faq.faq'|trans}}</h3>
                            <div class="faq-accordion accordion-style">
                                <div class="accordion" id="accordionExample2">
                                    <div class="single-accordion mb-30 wow fadeInUp" data-wow-delay=".3s">
                                        <div class="accordion-btn">
                                            <button class="btn-block text-left collapsed" type="button" data-toggle="collapse"
                                                    data-target="#collapse4" aria-expanded="true" aria-controls="collapse4">
                                                <span>{{'home.sections.contact.faq.whatIsCovid'|trans}}</span>
                                            </button>
                                        </div>

                                        <div id="collapse4" class="collapse" aria-labelledby="heading4" data-parent="#accordionExample2">
                                            <div class="accordion-content">
                                                {{'home.sections.contact.faq.whatIsCovidAnswer'|trans}}
                                            </div>
                                        </div>
                                    </div>
                                    <div class="single-accordion mb-30 wow fadeInUp" data-wow-delay=".4s">
                                        <div class="accordion-btn">
                                            <button class="btn-block text-left collapsed" type="button" data-toggle="collapse"
                                                    data-target="#collapse5" aria-expanded="true" aria-controls="collapse5">
                                                <span>{{'home.sections.contact.faq.whyIsCalledCovid'|trans}}</span>
                                            </button>
                                        </div>

                                        <div id="collapse5" class="collapse" aria-labelledby="heading5" data-parent="#accordionExample2">
                                            <div class="accordion-content">
                                                {{'home.sections.contact.faq.whyIsCalledCovidAnswer'|trans}}
                                            </div>
                                        </div>
                                    </div>
                                    <div class="single-accordion mb-30 wow fadeInUp" data-wow-delay=".5s">
                                        <div class="accordion-btn">
                                            <button class="btn-block text-left collapsed" type="button" data-toggle="collapse"
                                                    data-target="#collapse6" aria-expanded="true" aria-controls="collapse6">
                                                <span> {{'home.sections.contact.faq.howCovidSpread'|trans}}</span>
                                            </button>
                                        </div>

                                        <div id="collapse6" class="collapse" aria-labelledby="heading6" data-parent="#accordionExample2">
                                            <div class="accordion-content">
                                                {{'home.sections.contact.faq.howCovidSpreadAnswer'|trans}}
                                            </div>
                                        </div>
                                    </div>
                                    <div class="single-accordion mb-30 wow fadeInUp" data-wow-delay=".6s">
                                        <div class="accordion-btn">
                                            <button class="btn-block text-left collapsed" type="button" data-toggle="collapse"
                                                    data-target="#collapse7" aria-expanded="true" aria-controls="collapse7">
                                                <span>{{'home.sections.contact.faq.howCanIProtectMyself'|trans}}</span>
                                            </button>
                                        </div>

                                        <div id="collapse7" class="collapse" aria-labelledby="heading7" data-parent="#accordionExample2">
                                            <div class="accordion-content">
                                                {{'home.sections.contact.faq.howCanIProtectMyselfAnswer'|trans}}
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-6 col-lg-6">
                        <div class="contact-form-wrapper">
                            <h3 class="mb-40 wow fadeInUp" data-wow-delay=".3s">{{'home.sections.contact.contact.title'|trans}}</h3>
                            {{ form(form) }}
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <!--====== CONTACT PART ENDS ======-->
    </div>
{% endblock  %}
{% block javascripts %}
    {{ parent() }}
    <script type="text/javascript" src="{{ asset('build/home.js') }}"></script>
{% endblock %}