<!--====== HEADER PART START ======-->

<header class="header_area">
    <div id="header_navbar" class="header_navbar">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <nav class="navbar navbar-expand-lg">
                        <a class="navbar-brand" href="index.html">
                            <img id="logo" src="{{ asset('images/logo.svg') }}" alt="Logo">
                        </a>
                        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent"
                                aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                            <span class="toggler-icon"></span>
                            <span class="toggler-icon"></span>
                            <span class="toggler-icon"></span>
                        </button>
                        <div class="collapse navbar-collapse sub-menu-bar" id="navbarSupportedContent">
                            <ul id="nav" class="navbar-nav ml-auto">
                                <li class="nav-item">
                                    <a class="page-scroll active" href="#home">{{'home.menu.home' |trans}}</a>
                                </li>
                                <li class="nav-item">
                                    <a class="page-scroll" href="#keyNumbers">{{'home.menu.keyNumbers' |trans}}</a>
                                </li>
                                <li class="nav-item">
                                    <a class="page-scroll" href="#survey">{{'home.menu.survey' |trans}}</a>
                                </li>
                                <li class="nav-item">
                                    <a class="page-scroll" href="#situation">{{'home.menu.situation' |trans}}</a>
                                </li>
                                <li class="nav-item">
                                    <a class="page-scroll" href="#contact">{{'home.menu.contact' |trans}}</a>
                                </li>
                            </ul>
                        </div> <!-- navbar collapse -->
                    </nav> <!-- navbar -->
                </div>
            </div> <!-- row -->
        </div> <!-- container -->
    </div> <!-- header navbar -->
</header>

<!--====== HEADER PART ENDS ======-->

{% block content %}

{% endblock  %}

