<!doctype html>
<html class="no-js" lang="{{ app.request.locale|split('_')[0] }}">

<head>
    <meta charset="utf-8">

    <!--====== Title ======-->
    <title>{% block title %}cov-19.tn{% endblock %}</title>

    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!--====== Favicon Icon ======-->
    <link rel="icon" href="{{asset('images/favicon.png')}}" sizes="32x32">


    {% block stylesheets %}
        <link href="{{ asset('build/front-css.css') }}" rel="stylesheet" type="text/css"/>
    {% endblock %}
</head>

<body>
<!--[if IE]>
<p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="https://browsehappy.com/">upgrade your browser</a> to improve your experience and security.</p>
<![endif]-->

<!--====== PRELOADER PART START ======-->

<div class="preloader">
    <div class="loader">
        <div class="ytp-spinner">
            <div class="ytp-spinner-container">
                <div class="ytp-spinner-rotator">
                    <div class="ytp-spinner-left">
                        <div class="ytp-spinner-circle"></div>
                    </div>
                    <div class="ytp-spinner-right">
                        <div class="ytp-spinner-circle"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<!--====== PRELOADER PART ENDS ======-->

{% include 'front/_header.html.twig' %}

{% block content %}

{% endblock  %}

{% include 'front/_footer.html.twig' %}

<!--====== BACK TOP TOP PART START ======-->
<a href="#" class="back-to-top btn-hover"><i class="lni lni-chevron-up"></i></a>
<!--====== BACK TOP TOP PART ENDS ======-->


{% block javascripts %}
    <script type="text/javascript" src="{{ asset('build/front-js.js') }}"></script>
{% endblock %}
</body>

</html>