    <!--====== Title ======-->
    <title>{% block title %}cov-19.tn{% endblock %}</title>

    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!--====== Favicon Icon ======-->
    <link rel="icon" href="{{asset('images/favicon.png')}}" sizes="32x32">


    {% block stylesheets %}
